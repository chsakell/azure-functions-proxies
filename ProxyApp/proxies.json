{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "catalog.items": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/items"
      },
      "backendUri": "http://localhost:1072/api/items"
    },
    "catalog.item": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/items/{id}"
      },
      "backendUri": "http://localhost:1072/api/items/{id}"
    },
    "baskets.get": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/baskets/{id}"
      },
      "backendUri": "http://localhost:1073/api/baskets/{id}"
    },
    "baskets.update": {
      "matchCondition": {
        "methods": [ "PUT" ],
        "route": "/api/baskets"
      },
      "backendUri": "http://localhost:1073/api/baskets"
    },
    "baskets.delete": {
      "matchCondition": {
        "methods": [ "DELETE" ],
        "route": "/api/baskets/{id}"
      },
      "backendUri": "http://localhost:1073/api/baskets/{id}"
    },
    "orders.list": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/orders"
      },
      "backendUri": "http://localhost:1074/api/orders"
    }
  }
}